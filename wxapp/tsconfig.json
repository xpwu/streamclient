{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ES6",
    "target": "es6",
    "noImplicitReturns": true,
    "strict": true,
    "alwaysStrict": true,
    "pretty": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noEmit": false,
    "downlevelIteration": true,
    "noImplicitThis": true,
    "typeRoots": ["miniprogram-api-typings"],
    "lib":["ES6", "ScriptHost"]
  },
  "include": [
    "stream"
  ]
}